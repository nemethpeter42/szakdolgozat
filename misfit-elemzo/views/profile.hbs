{{#each data}}
	<h1>{{this.email}}</h1>
	{{#each this.subdata}}
		<h3>
			<ul>
				<li>
					{{@key}}
				</li>
			</ul>
		</h3>
		<table class="table table-striped table-hover ">
			{{#each this}}
				<tr>
					<td>
						<strong>
							{{@key}}
						</strong>
					</td>
					<td>
						{{this}}
					</td>
				</tr>
			{{/each}}
		</table>
	{{/each}}
{{/each}}